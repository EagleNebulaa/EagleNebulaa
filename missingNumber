class HelloWorld {
    public static void main(String[] args) {
        int[] arr = {1,2,3,4,6,7};
        int num = 7;
        int missing= num*(num+1)/2;
        int sum = 0;
        
        for(int n : arr) {
            sum = sum + n;
        }
        int rem = missing - sum;
        System.out.println(rem);
    }
}
